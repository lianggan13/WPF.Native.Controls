<Window
    x:Class="WPF.Template.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:WPF.Template"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:views="clr-namespace:WPF.Template.Views"
    Title="Template"
    Width="1385"
    Height="660"
    mc:Ignorable="d">
    <TabControl>
        <TabItem Header="ControlTemplateBrowser">
            <views:ControlTemplateBrowser />
        </TabItem>
        <TabItem Header="ContentTemplate">
            <views:ContentTemplate />
        </TabItem>
    </TabControl>
</Window>
