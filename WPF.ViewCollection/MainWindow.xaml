<Window
    x:Class="WPF.ViewCollection.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:WPF.ViewCollection"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:view="clr-namespace:WPF.ViewCollection.View"
    Title="MainWindow"
    Width="800"
    Height="450"
    WindowStartupLocation="CenterScreen"
    mc:Ignorable="d">
    <TabControl>
        <TabItem Header="SelectViewItem">
            <view:SelectViewItem />
        </TabItem>
        <TabItem Header="FilterViewCollection">
            <view:FilterViewCollection />
        </TabItem>
        <TabItem Header="GroupViewCollection">
            <view:GroupViewCollection />
        </TabItem>
        <TabItem Header="GroupInRangeViewCollection">
            <view:GroupInRangeViewCollection />
        </TabItem>
    </TabControl>

</Window>
