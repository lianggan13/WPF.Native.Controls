<Window
    x:Class="ColorFullProgressBar.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:ColorFullProgressBar"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="MainWindow"
    Width="800"
    Height="450"
    mc:Ignorable="d">
    <TabControl>
        <TabItem Header="AnimationProgressBar">
            <Frame Source="./View/AnimationProgressBar.xaml" />
        </TabItem>
        <TabItem Header="CornerProgressBar">
            <Frame Source="./View/CornerProgressBar.xaml" />
        </TabItem>
    </TabControl>

</Window>
